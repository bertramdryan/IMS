@inject IViewProductsByNameUseCase ViewProductByNameUseCase

@foreach(var prod in ListOfProducts)
{
    <tr>
        <td>@prod.ProductName</td>
        <td>@prod.Quantity</td>
        <td>@prod.Price.ToString("c")</td>
        <td>
            <button class="btn btn-primary">
                Edit
            </button>
        </td>
    </tr>
}


@code {
    [Parameter]
    public List<Product> ListOfProducts { get; set; } = new List<Product>();
}
